  "scripts": {
    "prestart": "npm run build:clean",
    "start": "webpack-dashboard -p 3333 -- webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run build:clean && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack --config ./config/webpack.config.prod.js --progress",
    "build:clean": "node ./bin/clean-build.js createdir",
    "build:html": "node ./bin/html-build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/towen/ramen.git"
  },
